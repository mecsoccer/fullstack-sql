{
  "name": "asteroid-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run migrate && npm run seed && node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "migrate": "node ./dist/db/migrate.js",
    "unmigrate": "node ./dist/db/unmigrate.js",
    "seed": "node ./dist/db/seeds/index.js",
    "migration": "npm run unmigrate && npm run migrate && npm run seed",
    "test": "npm run build && npm run migration && ts-mocha -p tsconfig.json --exit --timeout 20000 src/test/**/*.spec.ts",
    "lint": "eslint --ext .ts ./src",
    "lint-fix": "eslint --ext .ts ./src --fix"
  },
  "author": "Jaachimma Onyenze",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.2.1",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.0.4"
  }
